                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "showAllDataPoints": {
                    "displayName": "Show all",
                    "type": {
                        "bool": true
                    }
                },
                "fill": {
                    "displayName": "Fill",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fillRule": {
                    "displayName": "Color saturation",
                    "type": {
                        "fill": {}
                    }
                },
                "fontSize": {
                    "displayName": "Text Size",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }
            }
        },
        "xAxis": {
            "displayName": "X 坐标轴",
            "properties": {
                "show": {
                    "displayName": "显示坐标轴",
                    "type": {
                        "bool": true
                    }
                }
            }
        },
        "yAxis": {
            "displayName": "Y 坐标轴",
            "properties": {
                "show": {
                    "displayName": "显示坐标轴",
                    "type": {
                        "bool": true
                    }
                }
            }
        },
        "actual_dataColor": {
            "displayName": "实际数据颜色",
            "properties": {
                "fill": {
                    "displayName": "实际数据颜色",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        },
        "regression_dataColor": {
            "displayName": "拟合数据颜色",
            "properties": {
                "fill": {
                    "displayName": "拟合实际数据颜色",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    },
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
